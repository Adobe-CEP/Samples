				<UI>
					<Type>Panel</Type>
					<Menu>Transport Events</Menu>
					<Geometry>
						<Size>
							<Height>100</Height>
							<Width>150</Width>
						</Size>
					</Geometry>
				</UI>
			</DispatchInfo>
		</Extension>
